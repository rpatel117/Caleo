{
  "manifest_version": 2,
  "name": "Caleo",
  "version": "1.0",
  "description": "A Chrome Extension built with React.",
  "permissions": [],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/caleo.png"
    }
  },
  "icons": {
    "16": "assets/caleo.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
